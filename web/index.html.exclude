                                                <option value="gteq">>=</option>
                                            </select>
                                            <div class="invalid-feedback">
                                                Please select an operand.
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="qcThreshold"><i class="fas fa-asterisk fa-xs"></i> Threshold</label>
                                            <input id="qcThreshold" type="number" min="0" class="form-control" placeholder="Enter threshold" required="required">
