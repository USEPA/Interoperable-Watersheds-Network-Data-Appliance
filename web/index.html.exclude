            <div id="sensorModal" class="modal fade" tabindex="-1" role="dialog">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title">Add Sensor</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body needs-validation">
                            <div class="container-fluid">

                                <ul id="sensorModalTabs" class="nav nav-tabs" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link active" id="sensorInfoTab" data-toggle="tab" href="#sensorInfoTabContent" role="tab" aria-controls="sensorInfoTabContent" aria-selected="true">Sensor Information</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" id="sensorDataTab" data-toggle="tab" href="#sensorDataTabContent" role="tab" aria-controls="sensorDataContent" aria-selected="false">Sensor Data</a>
                                    </li>
                                </ul>
                                <div id="sensorModalTabsContent" class="tab-content pt-2">
                                    <div id="sensorInfoTabContent" class="tab-pane fade show active" role="tabpanel" aria-labelledby="sensorTab">

                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sendorId"><i class="fas fa-asterisk fa-xs"></i> Sensor ID</label>
                                                    <input id="sensorId" type="text" class="form-control" placeholder="Enter sensor ID" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor ID.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorNameShort"><i class="fas fa-asterisk fa-xs"></i> Short name</label>
                                                    <input id="sensorNameShort" type="text" class="form-control" placeholder="Enter sensor's short name" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor's short name.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label for="sensorNameLong"><i class="fas fa-asterisk fa-xs"></i> Long name</label>
                                                    <input id="sensorNameLong" type="text" class="form-control" placeholder="Enter sensor's long name" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor's long name.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <hr />
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="format-group">
                                                    <label for="sensorLatitude"><i class="fas fa-asterisk fa-xs"></i> Latitude (WGS84)</label>
                                                    <input id="sensorLatitude" type="number" min="-90" max="90" class="form-control" placeholder="Enter sensor's latitude" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor's latitude.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorLongitude"><i class="fas fa-asterisk fa-xs"></i> Longitude (WGS84)</label>
                                                    <input id="sensorLongitude" type="number" min="-180" max="180" class="form-control" placeholder="Enter sensor's longitude" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor's longitude.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorAltitude">Altitude (optional)</label>
                                                    <input id="sensorAltitude" type="number" min="0" class="form-control" placeholder="Enter sensor's altitude">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                            </div>
                                        </div>
                                        <hr />
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorTimeZone"><i class="fas fa-asterisk fa-xs"></i> Timezone</label>
                                                    <select id="sensorTimeZone" class="custom-select" required="required">
                                                        <option value="">Select sensor's timezone</option>
                                                        <option value="Pacific/Honolulu">(GMT-10:00) Hawaii</option>
                                                        <option value="US/Alaska">(GMT-09:00) Alaska</option>
                                                        <option value="America/Los_Angeles">(GMT-08:00) Pacific Time (US & Canada)</option>
                                                        <option value="US/Mountain">(GMT-07:00) Mountain Time (US & Canada)</option>
                                                        <option value="US/Central">(GMT-06:00) Central Time (US & Canada)</option>
                                                        <option value="US/Eastern">(GMT-05:00) Eastern Time (US & Canada)</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select the sensor's timezone.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorIngestFrequency"><i class="fas fa-asterisk fa-xs"></i> Ingest Frequency (minutes)</label>
                                                    <input id="sensorIngestFrequency" type="number" min="0" class="form-control" placeholder="Enter sensor's ingest frequency" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the sensor's ingest frequency.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="tab-pane fade" id="sensorDataTabContent" role="tabpanel" aria-labelledby="sensorDataTab">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorUrl"><i class="fas fa-asterisk fa-xs"></i> Data Location (URL)</label>
                                                    <input id="sensorUrl" type="text" class="form-control" placeholder="Enter data location (URL)" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the data location (URL).
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorQuality"><i class="fas fa-asterisk fa-xs"></i> Data Quality</label>
                                                    <select id="sensorQuality" class="custom-select" required="required">
                                                        <option value="">Select data quality</option>
                                                        <option value="raw">Raw</option>
                                                        <option value="prelim">Preliminary</option>
                                                        <option value="final">Final</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select the data quality.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="sensorResultTimeStamp"><i class="fas fa-asterisk fa-xs"></i> Timestamp Data Column</label>
                                                    <input id="sensorResultTimeStamp" class="form-control" placeholder="Enter timestamp data column" required="required">
                                                    <div class="invalid-feedback">
                                                        Please enter the timestamp data column.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6" style="padding-top:32px;">
                                                <div class="custom-control custom-checkbox">
                                                    <input id="sensorApplyQc" type="checkbox" class="custom-control-input">
                                                    <label class="custom-control-label" for="sensorApplyQc">Apply QC to Parameter Data</label>
                                                </div>
                                            </div>
                                        </div>
                                        <hr />
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h5>Parameter Data Columns</h5>
                                            </div>
                                            <div class="col-md-6">
                                                
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <label for="sensorParameter"><i class="fas fa-asterisk fa-xs"></i> Parameter</label>
                                                    <select id="sensorParameter" class="custom-select">
                                                        <option value="">Select a parameter</option>
                                                        <option value="79443">Barometric pressure</option>
                                                        <option value="42916">Chlorophyll</option>
                                                        <option value="80020">Colored dissolved organic matter (CDOM)</option>
                                                        <option value="79261">Conductivity</option>
                                                        <option value="79607">Depth</option>
                                                        <option value="78485">Dew point</option>
                                                        <option value="80050">Discharge</option>
                                                        <option value="101627">Dissolved Oxygen</option>
                                                        <option value="79586">Dissolved Oxygen Saturation</option>
                                                        <option value="79950">Escherichia coli</option>
                                                        <option value="79771">Flow</option>
                                                        <option value="79749">Flow, Total</option>
                                                        <option value="98309">Height, gage</option>
                                                        <option value="35904">Inorganic Nitrogen (Nitrate and Nitrite)</option>
                                                        <option value="80000">Lake or reservoir water surface elevation</option>
                                                        <option value="79002">Light, photosynthetic active radiation (PAR)</option>
                                                        <option value="79940">Nitrate as N</option>
                                                        <option value="79933">Oxidation Reduction Potential (ORP)</option>
                                                        <option value="79960">Petroleum Hydrocarbons</option>
                                                        <option value="78426">pH</option>
                                                        <option value="80010">Phosphate as P</option>
                                                        <option value="80040">Phosphate as PO4</option>
                                                        <option value="79970">Phycocyanin</option>
                                                        <option value="79912">Precipitation</option>
                                                        <option value="79436">Relative humidity</option>
                                                        <option value="79990">Reservoir storage</option>
                                                        <option value="79345">Salinity</option>
                                                        <option value="79373">Specific Conductance</option>
                                                        <option value="79287">Stream Velocity</option>
                                                        <option value="79246">Temperature, air</option>
                                                        <option value="79281">Temperature, Water</option>
                                                        <option value="79980">Total Suspended Solids</option>
                                                        <option value="79196">Turbidity</option>
                                                        <option value="79648">Wind direction</option>
                                                        <option value="78471">Wind gust, peak</option>
                                                        <option value="79645">Wind velocity</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <label for="sensorParameterColumn"><i class="fas fa-asterisk fa-xs"></i> Data Column</label>
                                                    <input id="sensorParameterColumn" class="form-control" placeholder="Enter the parameter's data column">
                                                </div>
                                            </div>
                                            <div class="col-md-2 text-right" style="padding-top:32px">
                                                <button type="button" class="btn btn-info">Add</button>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="table-responsive-md">
                                                    <table class="table table-striped" data-toggle="table">
                                                        <thead>
                                                            <tr>
                                                                <th class="text-info" data-sortable="true">Parameter</th>
                                                                <th class="text-info text-center" data-sortable="true">Data Column</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr>
                                                                <td class="text-left">Colored dissolved organic matter (CDOM)</td>
                                                                <td class="text-center">
                                                                    C
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td class="text-left">Dissolved Oxygen</td>
                                                                <td class="text-center">
                                                                    F
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="col-md-6 text-info text-left">
                                Fields marked with <i class="fas fa-asterisk fa-xs"></i> are required.
                            </div>
                            <div class="col-md-6 text-right">
                                <button type="button" class="btn btn-info">Save changes</button>
                                <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>

            <div id="qcModal" class="modal fade" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title">Quality Control</h5>
